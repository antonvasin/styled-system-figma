{
  "name": "styled-system-figma",
  "version": "0.1.2",
  "author": "Anton Vasin <antonvasin@me.com>",
  "description": "Generate styled-system theme from Figma file",
  "repository": "antonvasin/styled-system-figma",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "test": "ava"
  },
  "dependencies": {
    "color-namer": "^1.3.0",
    "figma-js": "^1.5.1"
  },
  "devDependencies": {
    "@types/color-namer": "^1.3.0",
    "ava": "^1.0.0-rc.2",
    "prettier": "^2.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "ava": {
    "files": "**/__tests__",
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
